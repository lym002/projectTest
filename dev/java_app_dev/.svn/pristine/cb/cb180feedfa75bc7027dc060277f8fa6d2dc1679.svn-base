package com.jsjf.model.member;

import java.math.BigDecimal;

public class DrMemberCredit {
	
	private Integer uid;

	/**
	 * 是否有房产
	 */
    private Boolean hasHouse; 

    
    /**
     * 是否有车产
     */
    private Boolean hasCar;

    /**
     * 逾期金额
     */
    private BigDecimal overdueAmount;

    /**
     * 其他贷款金额
     */
    private BigDecimal otherCreditAmount;
    
    /**
     * 信用卡额度
     */
    private BigDecimal creditCardAmount;

    /**
     * 待还款笔数
     */
    private Integer repayNum;

    /**
     * 按时还款笔数
     */
    private Integer ontimePayNum;

    /**
     * 提前还款笔数
     */
    private Integer advancePayNum;

    /**
     * 逾期笔数
     */
    private Integer lateNopayNum;

    /**
     * 垫后还款笔数
     */
    private Integer latePayNum;

    /**
     * 贷款总额
     */
    private BigDecimal creditCount;

    /**
     * 垫付金额
     */
    private BigDecimal receivedCount;
    
    /**
     * 待还本金
     */
    private BigDecimal aprincipal;

    /**
     * 待还利息
     */
    private BigDecimal ainterest;

    /**
     * 待还罚息
     */
    private BigDecimal apenalty;
    
    public DrMemberCredit(){}

    public DrMemberCredit(Integer uid, Boolean hasHouse, Boolean hasCar,
			BigDecimal overdueAmount, BigDecimal otherCreditAmount,
			BigDecimal creditCardAmount, BigDecimal creditCount,
			BigDecimal receivedCount, BigDecimal aprincipal,
			BigDecimal ainterest, BigDecimal apenalty) {
		super();
		this.uid = uid;
		this.hasHouse = hasHouse;
		this.hasCar = hasCar;
		this.overdueAmount = overdueAmount;
		this.otherCreditAmount = otherCreditAmount;
		this.creditCardAmount = creditCardAmount;
		this.creditCount = creditCount;
		this.receivedCount = receivedCount;
		this.aprincipal = aprincipal;
		this.ainterest = ainterest;
		this.apenalty = apenalty;
	}

	public Integer getUid() {
        return uid;
    }

    public void setUid(Integer uid) {
        this.uid = uid;
    }

    public Boolean getHasHouse() {
        return hasHouse;
    }

    public void setHasHouse(Boolean hasHouse) {
        this.hasHouse = hasHouse;
    }

    public Boolean getHasCar() {
        return hasCar;
    }

    public void setHasCar(Boolean hasCar) {
        this.hasCar = hasCar;
    }

    public BigDecimal getOverdueAmount() {
        return overdueAmount;
    }

    public void setOverdueAmount(BigDecimal overdueAmount) {
        this.overdueAmount = overdueAmount;
    }

    public BigDecimal getOtherCreditAmount() {
        return otherCreditAmount;
    }

    public void setOtherCreditAmount(BigDecimal otherCreditAmount) {
        this.otherCreditAmount = otherCreditAmount;
    }

    public BigDecimal getCreditCardAmount() {
        return creditCardAmount;
    }

    public void setCreditCardAmount(BigDecimal creditCardAmount) {
        this.creditCardAmount = creditCardAmount;
    }

    public Integer getRepayNum() {
        return repayNum;
    }

    public void setRepayNum(Integer repayNum) {
        this.repayNum = repayNum;
    }

    public Integer getOntimePayNum() {
        return ontimePayNum;
    }

    public void setOntimePayNum(Integer ontimePayNum) {
        this.ontimePayNum = ontimePayNum;
    }

    public Integer getAdvancePayNum() {
        return advancePayNum;
    }

    public void setAdvancePayNum(Integer advancePayNum) {
        this.advancePayNum = advancePayNum;
    }

    public Integer getLateNopayNum() {
        return lateNopayNum;
    }

    public void setLateNopayNum(Integer lateNopayNum) {
        this.lateNopayNum = lateNopayNum;
    }

    public Integer getLatePayNum() {
        return latePayNum;
    }

    public void setLatePayNum(Integer latePayNum) {
        this.latePayNum = latePayNum;
    }

    public BigDecimal getCreditCount() {
        return creditCount;
    }

    public void setCreditCount(BigDecimal creditCount) {
        this.creditCount = creditCount;
    }

    public BigDecimal getReceivedCount() {
        return receivedCount;
    }

    public void setReceivedCount(BigDecimal receivedCount) {
        this.receivedCount = receivedCount;
    }

    public BigDecimal getAprincipal() {
        return aprincipal;
    }

    public void setAprincipal(BigDecimal aprincipal) {
        this.aprincipal = aprincipal;
    }

    public BigDecimal getAinterest() {
        return ainterest;
    }

    public void setAinterest(BigDecimal ainterest) {
        this.ainterest = ainterest;
    }

    public BigDecimal getApenalty() {
        return apenalty;
    }

    public void setApenalty(BigDecimal apenalty) {
        this.apenalty = apenalty;
    }
}