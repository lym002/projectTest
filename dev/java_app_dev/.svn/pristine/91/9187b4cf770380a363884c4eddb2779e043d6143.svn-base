package com.jsjf.model.activity;

import java.math.BigDecimal;
import java.util.Date;

/**
 * 系统活动
 *
 */
public class JsActivityFriend {
	
	private Integer id;
	
	private String name;//活动名称
	
	private Integer conditionType; //奖励条件1-用户投资返现
	
	private Date startDate; //活动开始时间
	
	private Date endDate;//活动结束时间
	
	private Date addDate;//添加时间	
	
	private Integer status;//状态，1-进行中，2-已结束，默认为1
	
	private Integer periods;//期数
	
	private BigDecimal maxAmount;
	
	private Integer awardType;
	
	private Integer isPut;//推送至我的邀请，默认为1,0-不推送，1-推送
//	private String pcPutImg;//pc推送图片
//	private String pcPutUrl;//pc推送链接地址
	private String appPutImg;//app推送图片
	private String appPutUrl;//app推送链接地址
	private String putContent;//推送文案
	
	public JsActivityFriend(){}
	
	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Integer getConditionType() {
		return conditionType;
	}

	public void setConditionType(Integer conditionType) {
		this.conditionType = conditionType;
	}

	public Date getStartDate() {
		return startDate;
	}

	public void setStartDate(Date startDate) {
		this.startDate = startDate;
	}

	public Date getEndDate() {
		return endDate;
	}

	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}

	public Date getAddDate() {
		return addDate;
	}

	public void setAddDate(Date addDate) {
		this.addDate = addDate;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public Integer getPeriods() {
		return periods;
	}

	public void setPeriods(Integer periods) {
		this.periods = periods;
	}

	public BigDecimal getMaxAmount() {
		return maxAmount;
	}

	public void setMaxAmount(BigDecimal maxAmount) {
		this.maxAmount = maxAmount;
	}

	public Integer getAwardType() {
		return awardType;
	}

	public void setAwardType(Integer awardType) {
		this.awardType = awardType;
	}

	public Integer getIsPut() {
		return isPut;
	}

	public void setIsPut(Integer isPut) {
		this.isPut = isPut;
	}

	public String getAppPutImg() {
		return appPutImg;
	}

	public void setAppPutImg(String appPutImg) {
		this.appPutImg = appPutImg;
	}

	public String getAppPutUrl() {
		return appPutUrl;
	}

	public void setAppPutUrl(String appPutUrl) {
		this.appPutUrl = appPutUrl;
	}

	public String getPutContent() {
		return putContent;
	}

	public void setPutContent(String putContent) {
		this.putContent = putContent;
	}
	
	
}
