package com.jsjf.model.claims;

import java.math.BigDecimal;


public class DrClaimsShareholder{

    private Integer id;//股东情况ID 

    private Integer lid;//贷款项目基本信息ID
    
    private String name;//法人姓名

    private Integer sex;//性别

    private String phone;//手机号码
    
    private Integer type;//股东类别
    
    private Integer certificateType;//证件类型

    private String certificateNo;//证件号码
    
    private BigDecimal contributionLines;//出资额度

    private String contributionType;//出资方式

    private BigDecimal investRate;//投资比例

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getLid() {
		return lid;
	}

	public void setLid(Integer lid) {
		this.lid = lid;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Integer getSex() {
		return sex;
	}

	public void setSex(Integer sex) {
		this.sex = sex;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public Integer getCertificateType() {
		return certificateType;
	}

	public void setCertificateType(Integer certificateType) {
		this.certificateType = certificateType;
	}

	public String getCertificateNo() {
		return certificateNo;
	}

	public void setCertificateNo(String certificateNo) {
		this.certificateNo = certificateNo;
	}

	public BigDecimal getContributionLines() {
		return contributionLines;
	}

	public void setContributionLines(BigDecimal contributionLines) {
		this.contributionLines = contributionLines;
	}

	public String getContributionType() {
		return contributionType;
	}

	public void setContributionType(String contributionType) {
		this.contributionType = contributionType;
	}

	public BigDecimal getInvestRate() {
		return investRate;
	}

	public void setInvestRate(BigDecimal investRate) {
		this.investRate = investRate;
	}
    
}