package com.jsjf.model.member;

import java.math.BigDecimal;
import java.util.Date;

public class JsThreeFundSituation {

	private Integer id;
	private Date datetime;
	private BigDecimal investAmount;
	private BigDecimal rechargeAmount;
	private BigDecimal incomeAmount;
	private BigDecimal standbyRechargeAmount;
	private BigDecimal loanAmount;
	private BigDecimal withdrawalAmount;
	private BigDecimal poundageAmount;
	private BigDecimal standbyWithdrawalAmount;
	private BigDecimal activityAmount;
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Date getDatetime() {
		return datetime;
	}
	public void setDatetime(Date datetime) {
		this.datetime = datetime;
	}
	public BigDecimal getInvestAmount() {
		return investAmount;
	}
	public void setInvestAmount(BigDecimal investAmount) {
		this.investAmount = investAmount;
	}
	public BigDecimal getRechargeAmount() {
		return rechargeAmount;
	}
	public void setRechargeAmount(BigDecimal rechargeAmount) {
		this.rechargeAmount = rechargeAmount;
	}
	public BigDecimal getIncomeAmount() {
		return incomeAmount;
	}
	public void setIncomeAmount(BigDecimal incomeAmount) {
		this.incomeAmount = incomeAmount;
	}
	public BigDecimal getStandbyRechargeAmount() {
		return standbyRechargeAmount;
	}
	public void setStandbyRechargeAmount(BigDecimal standbyRechargeAmount) {
		this.standbyRechargeAmount = standbyRechargeAmount;
	}
	public BigDecimal getLoanAmount() {
		return loanAmount;
	}
	public void setLoanAmount(BigDecimal loanAmount) {
		this.loanAmount = loanAmount;
	}
	public BigDecimal getWithdrawalAmount() {
		return withdrawalAmount;
	}
	public void setWithdrawalAmount(BigDecimal withdrawalAmount) {
		this.withdrawalAmount = withdrawalAmount;
	}
	public BigDecimal getPoundageAmount() {
		return poundageAmount;
	}
	public void setPoundageAmount(BigDecimal poundageAmount) {
		this.poundageAmount = poundageAmount;
	}
	public BigDecimal getStandbyWithdrawalAmount() {
		return standbyWithdrawalAmount;
	}
	public void setStandbyWithdrawalAmount(BigDecimal standbyWithdrawalAmount) {
		this.standbyWithdrawalAmount = standbyWithdrawalAmount;
	}
	public BigDecimal getActivityAmount() {
		return activityAmount;
	}
	public void setActivityAmount(BigDecimal activityAmount) {
		this.activityAmount = activityAmount;
	}
	
}
