package com.jsjf.model.system;
public class SysRoleVo implements java.io.Serializable {
	private static final long serialVersionUID = -1795765791898512970L;
	private Long roleKy;//角色ID
    private String roleCode;//角色编码	
    private String roleName;//角色名称
    private Short status;//角色状态
    private String roleRemarks;//角色备注
    private Integer id;//
    private String isChecked="";//选中权限
    
    public String getIsChecked() {
		return isChecked;
	}

	public void setIsChecked(String isChecked) {
		this.isChecked = isChecked;
	}

    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

    public Long getRoleKy() {
        return roleKy;
    }

    public void setRoleKy(Long roleKy) {
        this.roleKy = roleKy;
    }

    public String getRoleCode() {
        return roleCode;
    }


    public void setRoleCode(String roleCode) {
        this.roleCode = roleCode;
    }

    public String getRoleName() {
        return roleName;
    }

    public void setRoleName(String roleName) {
        this.roleName = roleName.trim();
    }

    public Short getStatus() {
        return status;
    }

    public void setStatus(Short status) {
        this.status = status;
    }

    public String getRoleRemarks() {
        return roleRemarks;
    }

    public void setRoleRemarks(String roleRemarks) {
        this.roleRemarks = roleRemarks;
    }
}