package com.jsjf.model.claims;

import java.math.BigDecimal;
import java.util.Date;

public class JsLoanRecord {
	
	public Integer id;//
	public Integer lid;//债权id
	public Integer pid;//产品id
	public String contractCode;//借款合同编号
	public String claimsName;//资产名称
	public String company;//企业名称
	public BigDecimal amount;//借款金额
	public String loanName;//贷款户名
	public String bankName;//开户行
	public String bankNo;//卡号
	public String productName;//产品名称
	public Date addtime;//添加时间
	public Integer addUseky;//添加人
	
	public Date startTime;//开始时间
	public Date endTime;//结束时间
	public Integer claimsLoanStauts;//债权当前状态
	
	public JsLoanRecord(){
		super();
	}
	
	public JsLoanRecord(Integer lid, Integer pid, String contractCode,
			String claimsName, String company, BigDecimal amount,
			String loanName, String bankName, String bankNo,
			String productName, Integer addUseky,Date startTime,Date endTime,Integer claimsLoanStauts) {
		super();
		this.lid = lid;
		this.pid = pid;
		this.contractCode = contractCode;
		this.claimsName = claimsName;
		this.company = company;
		this.amount = amount;
		this.loanName = loanName;
		this.bankName = bankName;
		this.bankNo = bankNo;
		this.productName = productName;
		this.addtime = new Date();
		this.addUseky = addUseky;
		this.startTime = startTime;
		this.endTime = endTime;
		this.claimsLoanStauts = claimsLoanStauts;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	
	public Integer getLid() {
		return lid;
	}
	public void setLid(Integer lid) {
		this.lid = lid;
	}
	public Integer getPid() {
		return pid;
	}
	public void setPid(Integer pid) {
		this.pid = pid;
	}
	public String getContractCode() {
		return contractCode;
	}
	public void setContractCode(String contractCode) {
		this.contractCode = contractCode;
	}
	public String getClaimsName() {
		return claimsName;
	}
	public void setClaimsName(String claimsName) {
		this.claimsName = claimsName;
	}
	public String getCompany() {
		return company;
	}
	public void setCompany(String company) {
		this.company = company;
	}
	public BigDecimal getAmount() {
		return amount;
	}
	public void setAmount(BigDecimal amount) {
		this.amount = amount;
	}
	public String getLoanName() {
		return loanName;
	}
	public void setLoanName(String loanName) {
		this.loanName = loanName;
	}
	public String getBankName() {
		return bankName;
	}
	public void setBankName(String bankName) {
		this.bankName = bankName;
	}
	public String getBankNo() {
		return bankNo;
	}
	public void setBankNo(String bankNo) {
		this.bankNo = bankNo;
	}
	public String getProductName() {
		return productName;
	}
	public void setProductName(String productName) {
		this.productName = productName;
	}
	public Date getAddtime() {
		return addtime;
	}
	public void setAddtime(Date addtime) {
		this.addtime = addtime;
	}
	public Integer getAddUseky() {
		return addUseky;
	}
	public void setAddUseky(Integer addUseky) {
		this.addUseky = addUseky;
	}

	public Date getStartTime() {
		return startTime;
	}

	public void setStartTime(Date startTime) {
		this.startTime = startTime;
	}

	public Date getEndTime() {
		return endTime;
	}

	public void setEndTime(Date endTime) {
		this.endTime = endTime;
	}

	public Integer getClaimsLoanStauts() {
		return claimsLoanStauts;
	}

	public void setClaimsLoanStauts(Integer claimsLoanStauts) {
		this.claimsLoanStauts = claimsLoanStauts;
	}

	
}
